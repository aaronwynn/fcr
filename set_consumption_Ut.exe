func make_positive_float_over_length(s string, l int64) float64 {
	for i := l - 1; i >= 0; i-- {
		_, err := strconv.Atoi(s[i : i+1])
		if err == nil {
			erg, err2 := strconv.ParseFloat(s[:i+1], 64)
			if err2 != nil {
				fmt.Println("ERROR")
			}
			return erg
		}
	}
	return -1
}

